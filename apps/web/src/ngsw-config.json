{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(svg|cur|jpg|jpeg|png|webp|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-freshness",
      "urls": [
        "/api/orders/**",
        "/api/orders/*/complete",
        "/api/orders/*/waste"
      ],
      "cacheConfig": {
        "maxSize": 500,
        "maxAge": "1h",
        "timeout": "5s",
        "strategy": "freshness"
      }
    },
    {
      "name": "api-performance",
      "urls": [
        "/api/metadata/installers",
        "/api/metadata/waste-codes",
        "/api/health"
      ],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "1d",
        "strategy": "performance"
      }
    }
  ],
  "navigationUrls": [
    "/**",
    "!/**/*.{js,css,json,woff,woff2,jpg,jpeg,png,gif,svg}"
  ]
}
